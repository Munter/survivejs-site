{
  "name": "survivejs-site",
  "private": true,
  "version": "0.1.0",
  "description": "SurviveJS site",
  "author": "Juho Vepsalainen <bebraw@gmail.com>",
  "scripts": {
    "start": "antwar -d",
    "build": "antwar -b && npm run sitemap",
    "deploy": "antwar -D && npm run purge_cf",
    "sitemap": "cd build && sitemap-static --prefix=http://survivejs.com/ > sitemap.xml",
    "purge_cf": "node ./utils/purge_cf.js"
  },
  "dependencies": {
    "react-ghfork": "^0.3.0"
  },
  "devDependencies": {
    "antwar-default-theme": "git://github.com/antwarjs/default-theme",
    "antwar-highlight-plugin": "git://github.com/antwarjs/highlight-plugin",
    "antwar-prevnext-plugin": "git://github.com/antwarjs/prevnext-plugin",
    "antwar-rss-plugin": "antwarjs/rss-plugin",
    "cloudflare4": "^0.2.2",
    "lodash": "^3.9.1",
    "marked": "^0.3.3",
    "raw-loader": "^0.5.1",
    "remove-markdown": "0.0.2",
    "sitemap-static": "^0.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/survivejs/site.git"
  },
  "homepage": "https://www.survivejs.com",
  "bugs": {
    "url": "https://github.com/survivejs/site/issues"
  },
  "keywords": [
    "site",
    "antwar"
  ],
  "licenses": "MIT"
}
